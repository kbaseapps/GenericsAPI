      "text_options": {
          "validate_as": "float"
      }
    },
    {
      "id": "abundance_filtering_columns_sum_threshold",
      "optional":false,
      "advanced":true,
      "allow_multiple":false,
      "default_values":["10000"],
      "field_type" : "text",
      "text_options": {
          "validate_as": "float"
      }
