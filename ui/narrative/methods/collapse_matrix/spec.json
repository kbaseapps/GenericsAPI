{
  "ver" : "",
  "authors" : ["tgu2"],
  "contact" : "http://kbase.us/contact-us/",
  "visible" : true,
  "categories" : ["active","util"],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output" : "no-display"
  },
  "parameters" : [
    {
      "id" : "input_matrix_ref",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseMatrices.AmpliconMatrix"]
      }
    },
    {
      "id": "taxonomy_field",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [ "" ],
      "field_type": "textsubdata",
      "textsubdata_options": {
        "subdata_selection": {
           "parameter_id": "input_matrix_ref",
           "path_to_subdata": [
               "row_attributemapping_ref",
               "<WSREF>",
               "attributes"
           ],
           "subdata_included": [
               "attributes/[*]/attribute",
               "attributes/[*]/source",
               "row_attributemapping_ref"
           ],
           "selection_id": "attribute",
           "selection_description": [
               "attribute",
               "source"
           ],
           "description_template": "source: {{source}}"
        },
        "multiselection": false,
        "show_src_obj": true,
        "allow_custom": false
      }
    },
    {
      "id" : "taxonomy_rank",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : ["Class"],
      "field_type" : "dropdown",
      "dropdown_options":
      {
        "options":
        [
          {
              "display": "Domain",
              "value": "Domain"
          },
          {
              "display": "Phylum",
              "value": "Phylum"
          },
          {
              "display": "Class",
              "value": "Class"
          },
          {
              "display": "Order",
              "value": "Order"
          },
          {
              "display": "Family",
              "value": "Family"
          },
          {
              "display": "Genus",
              "value": "Genus"
          }
        ]
      }
    },
    {
      "id" : "new_matrix_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
          "valid_ws_types" : ["KBaseMatrices.AmpliconMatrix"],
          "is_output_name" : true
        }
    }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "GenericsAPI",
      "method" : "perform_rarefy",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace_id",
          "target_property": "workspace_id"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter" : "input_matrix_ref",
          "target_property" : "input_matrix_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "taxonomy_field",
          "target_property" : "taxonomy_field"
        },
        {
          "input_parameter" : "taxonomy_rank",
          "target_property" : "taxonomy_rank"
        },
        {
          "input_parameter" : "new_matrix_name",
          "target_property" : "new_matrix_name"
        }
      ],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
            "service_method_output_path": [0, "new_matrix_obj_ref"],
            "target_property": "new_matrix_obj_ref"
        },
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        },
        {
            "constant_value": "16",
            "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
